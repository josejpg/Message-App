<VBox prefHeight="210.0" prefWidth="281.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="message.app.Login">
  <children>
    <AnchorPane centerShape="false" maxHeight="-Infinity" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="211.0" prefWidth="345.0" VBox.vgrow="ALWAYS">
      <children>
