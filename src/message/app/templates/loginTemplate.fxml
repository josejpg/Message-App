            <Label id="lblPassword" fx:id="lblPassword" layoutX="45.0" layoutY="79.0" text="Password" />
            <PasswordField id="txtPassword" fx:id="txtPassword" layoutX="146.0" layoutY="75.0" promptText="Password" />
            <TextField id="txtUser" fx:id="txtUser" layoutX="146.0" layoutY="45.0" promptText="User" />
